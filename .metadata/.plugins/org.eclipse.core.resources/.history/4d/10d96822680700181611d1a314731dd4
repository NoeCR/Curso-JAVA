import java.util.Arrays;
import java.util.Random;
import java.util.Scanner;
public class Ejercicio6 {
    public static void rellenaArray(int[] a, int max){
        Random aleatorio = new Random(System.currentTimeMillis());
        // Producir nuevo int aleatorio entre 0 y (max-1)
        int intAleatorio;
        for (int i = 0; i < a.length; i++){
            a[i] = aleatorio.nextInt(max);
        }
    }
    public static void main(String[] args) {
      int[] array1 = new int[10];
      rellenaArray(array1, 30);
      int[] array2 = array1.clone();
      boolean comp = false;
      System.out.println("Array : "+Arrays.toString(array1));
      System.out.println("Array : "+Arrays.toString(array2));

      for(int i=0; i<array1.length;i++){
        if(array1[i] != array2[i]){
          comp = true;
          break;
        }
      }
      if(comp){
        System.out.println("Las cadenas no coinciden");
      }else{
        System.out.println("Las cadenas coinciden");
      }
    }
}
